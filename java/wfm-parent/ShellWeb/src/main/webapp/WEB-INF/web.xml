<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Spring Web MVC Application</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ProcessServlet</display-name>
    <servlet-name>ProcessServlet</servlet-name>
    <servlet-class>com.nice.wfm.env.tool.ProcessServlet</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>ProcessServlet</display-name>
    <servlet-name>ProcessServlet</servlet-name>
    <servlet-class>com.nice.wfm.env.tool.CheckFileServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ProcessServlet</servlet-name>
    <url-pattern>/run</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProcessServlet</servlet-name>
    <url-pattern>/check</url-pattern>
  </servlet-mapping>
</web-app>